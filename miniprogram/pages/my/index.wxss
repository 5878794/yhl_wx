
page{
    background: #f2f2f4;
}

.my_title{
    padding:0 30rpx;
    height: 220rpx;
    background: #fff;
}
.my_title image{
    display: block;
    width: 130rpx;height: 130rpx;
    border-radius: 130rpx;
    border:8rpx solid #d9dadb;
}
.my_title_name{
    font-weight: bold;
    font-size: 34rpx;
    padding:0 20rpx;
}
.my_title .btn{
    border-radius: 60rpx;
    padding:0 30rpx;
    width: 270rpx; height: 77rpx;
    font-size:24rpx;
    color:rgba(203,243,251,1);
}
.my_money{
    font-size:40rpx;
    font-weight:bold;
    color:rgba(255,255,255,1);
    padding-left: 10rpx;
}

.my_title_row{
    padding:0 30rpx;
    height: 110rpx;
    background: #f2f2f4;
    font-size: 32rpx; color: #333;
    font-weight: bold;
}
.my_title_row image{
    display: block; width: 49rpx; height: 43rpx;
    margin-right: 20rpx;
}
.my_item{
    padding:0 30rpx;
    background: #f2f2f4;
    font-size:24rpx;
    font-weight:400;
    color:rgba(69,69,69,1);
    margin-bottom: 40rpx;
}
.my_item text{
    font-weight: bold;
    color: #333;
    padding-left:10rpx;
    text-align: right;
}
.my_item_body{
    width: 100%;
    padding:10rpx 30rpx;
    background: #fff;
    box-shadow:0rpx 0rpx 18rpx 0rpx rgba(0, 0, 0, 0.15);
    border-radius:8rpx;
}
.my_item_title{
    width: 100%; height: 80rpx;
    border-bottom:4rpx solid rgba(232,232,232,1);
}
.my_item_row{
    width: 100%; height: 60rpx;
    border-bottom:1rpx solid rgba(232,232,232,1);
}
.my_item_row:last-child{
    border-bottom:none;
}
.my_item_row_text image{
    width: 44rpx; height: 34rpx; display: block;
    margin-left: 40rpx;
}
.my_item_row_text .copy{
    display: block; width: 44rpx; height: 44rpx;
}

.state1,
.state2,
.state3{
    text-align: right;
}
.state1{
    color:rgba(255,136,0,1);
}
.state2{
    color:rgba(31,169,57,1);
}
.state3{
    color:rgba(69,69,69,1);
}