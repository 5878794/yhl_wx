.warrant_search_item{
    width: 690rpx; margin: 20rpx auto;
    background: #f9fafb;
}
.warrant_search_item_title{
    padding:0 30rpx; height: 80rpx;
    border-bottom:4rpx solid rgba(232,232,232,1);
    font-size:24rpx;
    font-weight:400;
    color:rgba(69,69,69,1);
}
.warrant_search_row{
    border-bottom:1rpx solid rgba(239,239,239,1);
    padding:0 30rpx; height: 60rpx;
    font-size:24rpx;
    font-weight:400;
    color:rgba(69,69,69,1);
}



.warrant_search_item text{
    font-size:24rpx;
    font-weight:bold;
    color:rgba(0,0,0,1);
    text-align: right;
    padding-left:10rpx;
}