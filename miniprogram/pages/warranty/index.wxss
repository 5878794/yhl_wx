}
.warranty_pp_item{
    width: 25%; font-size:24rpx;
    height: 120rpx;
    font-weight:400;
    color:rgba(69,69,69,1);
    padding-bottom:20rpx;
    border-bottom:1rpx solid #eee;
    border-right:1rpx solid #eee;
}
