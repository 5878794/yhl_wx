//select
<b-select
    class="b_input"
    icon="../../images/5@1x.png"
    type="select"
    placeholder="电话号码"
    name="电话"
    id="name"
    selectData="{{selectData}}"         //[{key:11,value:'a'},...]
    value="{{key}}"                     //key的值
    rule="must"
    err="请输入用户名"
    bindmychange='function'
></b-select>

