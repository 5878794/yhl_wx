//需要 __publish 文件夹

//时间、日期  选择器

//动态改变值时未判断是否超出最小、最大范围

<b-date
    class="b_input"
    icon="../../images/5@1x.png"
    type="date"                     //date：日期控件   time：时间控件
    start=""            //最小值     //date: 2020-11-11  time:08:11
    end=""              //最大值
    placeholder="电话号码"
    name="电话"
    id="name"
    value=""                        //date:2020-11-11  time:05:11
    rule="must"
    err="请输入用户名"
    bindmychange='function'
></b-date>
